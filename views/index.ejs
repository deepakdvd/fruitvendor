<style>

  .slideanim {visibility:hidden;}
  .slide {
      animation-name: slide;
      -webkit-animation-name: slide;
      animation-duration: 1s;
      -webkit-animation-duration: 1s;
      visibility: visible;
  }
  @keyframes slide {
    0% {
      opacity: 0;
      transform: translateY(70%);
    } 
    100% {
      opacity: 1;
      transform: translateY(0%);
    }
  }
  @-webkit-keyframes slide {
    0% {
      opacity: 0;
      -webkit-transform: translateY(70%);
    } 
    100% {
      opacity: 1;
      -webkit-transform: translateY(0%);
    }
  }


  .main-glamorous {
    text-align: center;
    margin-bottom: 10vh;
    padding: 0 15px;
}
.main-glamorous .wrapper-img-glamorous {
    display: inline-block;
    position: relative;
}
.main-glamorous img {
    margin-left: auto;
    margin-right: auto;
    margin-bottom: 20px;
    transition: all 0.3s ease;
    -webkit-transition: all 0.3s ease;
    -moz-transition: all 0.3s ease;
    -o-transition: all 0.3s ease;
}
.main-glamorous .links-icons-gla {
    font-size: 1.6em;
    font-weight: bold;
    text-transform: uppercase;
    color: #125f06;
    margin-bottom: 10px;
    display: block;
}
.main-glamorous .text-gla {
    margin-bottom: 0;
}
.text-gla{
  font-size: 1.1em;
}

.main-glamorous:hover .wrapper-img-glamorous:before {
    transform: scale(1);
    -webkit-transform: scale(1);
    -moz-transform: scale(1);
    -ms-transform: scale(1);
    -o-transform: scale(1);
}
.main-glamorous .wrapper-img-glamorous:before {
    content: "";
    position: absolute;
    bottom: 0;
    left: 0;
    right: 0;
    width: 70%;
    height: 15px;
    margin-left: auto;
    margin-right: auto;
    background-color: #125f06;
    border-radius: 50%;
    opacity: 0.5;
    transform: scale(1, 0);
    -webkit-transform: scale(1, 0);
    -moz-transform: scale(1, 0);
    -ms-transform: scale(1, 0);
    -o-transform: scale(1, 0);
    transition: all 0.4s ease;
    -webkit-transition: all 0.4s ease;
    -moz-transition: all 0.4s ease;
    -o-transition: all 0.4s ease;
}
